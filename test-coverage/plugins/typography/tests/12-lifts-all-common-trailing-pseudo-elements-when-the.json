{
    "name": "lifts all common, trailing pseudo elements when the same across all selectors",
    "html": "<div class=\"prose dark:prose\"></div>",
    "config": {
        "content": "{\n        raw: html`<div class=\"prose dark:prose\"></div>`,\n      },",
        "hasTypographyTheme": true
    },
    "pluginOptions": [],
    "expectedCss": [
        {
            "type": "include",
            "css": ".prose :where(ol li, ul li):not(:where([class~='not-prose'], [class~='not-prose'] *))::marker::before { color: red; }"
        },
        {
            "type": "include",
            "css": ".dark .dark\\:prose :where(ol li, ul li):not(:where([class~='not-prose'], [class~='not-prose'] *))::before::marker { color: red; }"
        }
    ]
}