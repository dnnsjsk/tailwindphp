{
    "name": "should not break with multiple selectors with pseudo elements using variants",
    "html": "<div class=\"dark:prose\"></div>",
    "config": {
        "content": "{\n        raw: html`<div class=\"dark:prose\"></div>`,\n      },",
        "typography": {
            "DEFAULT": {
                "css": [
                    {
                        "ol li::before, ul li::before": {
                            "color": "red"
                        }
                    }
                ]
            }
        },
        "darkMode": "class"
    },
    "pluginOptions": [],
    "expectedCss": [
        {
            "type": "include",
            "css": ".dark .dark\\:prose :where(ol li, ul li):not(:where([class~='not-prose'], [class~='not-prose'] *))::before { color: red; }"
        }
    ]
}