{
    "name": "should be possible to use nested syntax (&) when extending the config",
    "html": "<div class=\"prose\"></div>",
    "config": {
        "content": "{\n        raw: html`<div class=\"prose\"></div>`,\n      },",
        "typography": {
            "DEFAULT": {
                "css": [
                    {
                        "color": "#000",
                        "a": {
                            "color": "#888",
                            "&:hover": {
                                "color": "#ff0000"
                            }
                        }
                    }
                ]
            },
            "_isExtend": true
        }
    },
    "pluginOptions": [],
    "expectedCss": [
        {
            "type": "include",
            "css": ".prose { color: #000; max-width: 65ch; }"
        },
        {
            "type": "include",
            "css": ".prose :where(a):not(:where([class~='not-prose'], [class~='not-prose'] *)) { color: #888; text-decoration: underline; font-weight: 500; }"
        },
        {
            "type": "include",
            "css": ".prose :where(a):not(:where([class~='not-prose'], [class~='not-prose'] *)):hover { color: #ff0000; }"
        }
    ]
}